{
  "name": "react-dom-html",
  "version": "1.3.0",
  "description": "The easiest way to render Html applications using react",
  "repository": "git@github.com:adam-26/react-dom-html.git",
  "author": "Adam <adam-26@users.noreply.github.com>",
  "bugs": {
    "url": "https://github.com/adam-26/react-dom-html/issues"
  },
  "homepage": "https://github.com/adam-26/react-dom-html/packages/react-dom-html",
  "license": "MIT",
  "main": "lib/index.js",
  "modules": "lib/index.es.js",
  "js:next": "lib/index.es.js",
  "engines": {
    "node": ">=4.8"
  },
  "scripts": {
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "precommit": "pretty-quick --staged",
    "clean": "rm -rf ./lib",
    "prebuild": "npm run clean",
    "build": "npm run build:lib",
    "build:lib": "rollup -c rollup.config.lib.js",
    "postbuild:lib": "cp ./src/exports/* ./lib/",
    "lint": "eslint ./src",
    "ci-jest": "TEST_REPORT_PATH=./.build/coverage/jest jest --ci --coverage --coverageDirectory ./.build/coverage/jest --testResultsProcessor='../../node_modules/jest-junit-reporter'",
    "ci-lint": "eslint ./src --format junit --output-file ./.build/lint/eslint.xml",
    "preci-test": "yarn run build",
    "ci-test": "yarn run ci-jest && yarn run test:browser",
    "postci-test": "yarn run mergeCoverageReports",
    "pretest": "yarn run clean && yarn run build",
    "test": "yarn run test:server && yarn run test:browser",
    "posttest": "yarn run mergeCoverageReports",
    "test:server": "jest",
    "posttest:server": "cp ./test-report.xml ./.build/coverage/jest-test-report.xml",
    "test:browser": "cross-env BABEL_ENV=test karma start karma.config.js",
    "posttest:browser": "istanbul report --root ./.build/coverage/karma/json --dir ./.build/coverage/karma lcov text",
    "mergeCoverageReports": "../../node_modules/.bin/junit-merge ./.build/coverage/karma/junit/karma-results.xml ./.build/coverage/jest/test-report.xml -o ./.build/coverage/merged-results.xml"
  },
  "keywords": [
    "react",
    "react-dom",
    "html",
    "react-dom-html"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-minify-dead-code-elimination": "^0.4.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.8",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.20.0",
    "chai": "^3.5.0",
    "codecov": "^2.1.0",
    "cross-env": "^5.1.4",
    "eslint": "^4.12.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.46.2",
    "eslint-plugin-jest": "^21.4.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.69.0",
    "history": "^4.7.2",
    "istanbul": "^0.4.5",
    "jest": "^22.4.3",
    "jest-junit-reporter": "^1.1.0",
    "jsdom": "^11.8.0",
    "junit-merge": "^1.3.0",
    "karma": "^1.5.0",
    "karma-chai": "^0.1.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-html-reporter": "^0.2.7",
    "karma-junit-reporter": "^1.2.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-phantomjs-shim": "^1.4.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.30",
    "karma-tap-reporter": "^0.0.6",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.2.0",
    "phantomjs-prebuilt": "^2.1.14",
    "prettier": "^1.12.1",
    "pretty-quick": "^1.4.1",
    "prop-types": "^15.6.0",
    "rc-util": "^4.5.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-reconciler": "^0.9.0",
    "react-test-renderer": "^16.3.2",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-filesize": "^1.5.0",
    "rollup-plugin-minify": "^1.0.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.8.0",
    "standard-version": "^4.2.0",
    "stream-to-string": "^1.1.0",
    "webpack": "^2.2.1"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "dependencies": {
    "invariant": "^2.2.2"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "coverageDirectory": "./.build/coverage/jest",
    "collectCoverageFrom": [
      "src/util.js",
      "src/server/*.js"
    ],
    "testMatch": [
      "**/__tests__/*.test.js?(x)"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/src/__tests__/reactSixteenAdapter.js"
    ],
    "testResultsProcessor": "../../node_modules/jest-junit-reporter"
  }
}
